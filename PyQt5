import sys
from PyQt5.QtWidgets import QApplication, QWidget

def main():

    app = QApplication(sys.argv)
    w = QWidget()
    w.resize(500, 500)
    w.move(300, 300)
    w.setWindowTitle('PyQt5')
    w.show()

    sys.exit(app.exec_())

if __name__ == '__main__':
    main()
#this will create a tiny window named 'PyQt5' with dimensions 500*500
